{
    "name": "Web3 Wallet",
    "description": "Transfer funds to any wallet",
    "version": "1.0",
    "manifest_version": 3,  
    "action": {
        "default_popup": "popup.html",
        "default_icon": "favicon.png"
    },
    "icons": {
        "16": "favicon.png",
        "32": "favicon.png",
        "48": "favicon.png",
        "128": "favicon.png"
    },
    "permissions": ["tabs","bookmarks","unlimitedStorage","nativeMessaging"]
}